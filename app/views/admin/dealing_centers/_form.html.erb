
<%= error_messages_for :dealing_center %>
<%form_for [:admin,@dealing_center], :html => { :multipart => true } do |f|%>

<p>
<%= f.label 'Название' %><br />
<%=f.text_field :name%>
</p>

<p>
<%= f.label 'Описание ДЦ' %><br />
<%=f.text_area :description, :size=>"70x6"%>
</p>

<p>
<%=f.submit 'Сохранить изменения' %>
</p>
<%end%>